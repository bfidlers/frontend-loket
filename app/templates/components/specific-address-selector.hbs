{{#power-select
   allowClear=allowClear
   disabled=disabled
   placeholder=placeholder
   selected=_address
   options=options
   onchange=(action "select")
   loadingMessage="Aan het laden..."
   noMatchesMessage="Geen resultaten"
   as |address|}}
  {{#if address.busnummer}}
    Bus {{address.busnummer}}
  {{else}}
    Geen busnummer
  {{/if}}
{{/power-select}}
