<Mandatenbeheer::PowerSelect
        @searchEnabled={{true}}
        @allowClear={{@allowClear}}
        @disabled={{@disabled}}
        @placeholder="Titelvoerend, effectief, waarnemend, verhinderd"
        @options={{this.statusCodeList}}
        @selected={{this.selectedStatusCode}}
        @searchField="label"
        @onChange={{this.select}}
        as |status|>
  {{status.label}}
</Mandatenbeheer::PowerSelect>
