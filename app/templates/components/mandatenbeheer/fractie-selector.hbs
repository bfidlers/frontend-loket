<Mandatenbeheer::PowerSelect
        @searchEnabled={{true}}
        @allowClear={{@allowClear}}
        @disabled={{@disabled}}
        @placeholder="Zoek fractie"
        @search={{perform this.searchByName}}
        @selected={{this._fractie}}
        @onChange={{this.select}}
        as |fractie|>
  {{fractie.naam}}
</Mandatenbeheer::PowerSelect>
