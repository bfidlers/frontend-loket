<PowerSelect
  @id="worship-minister-position-select"
  @noMatchesMessage="Geen resultaten"
  @searchMessage="Typ om te zoeken"
  @placeholder="Selecteer een functie"
  @options={{@options}}
  @selected={{@selected}}
  @onChange={{fn (mut @selected)}}
  as |ministerPosition|
  >
  {{ministerPosition.function.label}}
</PowerSelect>